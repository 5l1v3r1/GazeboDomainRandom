<launch>
  <arg name="box_name" default="BOX0"/>
	<arg name="box_color" default="Orange"/>
	<arg name="X" default="2.0"/>
	<arg name="Y" default="0.0"/>
	<arg name="Z" default="0.3"/>

	<!-- Convert an xacro and put on parameter server -->
	<param name="robot_description" command="$(find xacro)/xacro.py '$(find GazeboDomainRandom)/urdf/box.urdf.xacro' name:='$(arg box_name)' color:='$(arg box_color)'" />
  
  <!-- Spawn a model into Gazebo -->
	<node name="spawn_box_$(arg box_name)" pkg="gazebo_ros" type="spawn_model" args="-param robot_description -urdf -z $(arg Z) -x $(arg X) -y $(arg Y) -model $(arg box_name)" />
	
</launch>
